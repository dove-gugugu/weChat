/* miniprogram/pages/map/map.wxss */
.view {
  height: auto;
  width: auto;
}
.newmap {
  height: 100vh;
  width: 100vh;
  z-index: 9000;
}
.dingwei {
  display: table-cell;
  height: 55rpx;
  width: 55rpx;
  vertical-align: middle;
  text-align: center;
}

.dingwei-img {
  height: 55rpx;
  width: 55rpx;
}
.mask{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  z-index: 9900;
  opacity: 0.7;
}

.modalDlg{
  width: 580rpx;
  height: 620rpx;
  position: fixed;
  top: 50%;
  left: 0;
  z-index: 9999;
  margin: -370rpx 85rpx;
  background-color: #fff;
  border-radius: 36rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}